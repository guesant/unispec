{
  "name": "@unispec/driver-nestjs",
  "version": "0.26.0",
  "type": "module",
  "sideEffects": false,
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "release:base": "release-it -c .release-it.json",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "coverage": "vitest run --coverage",
    "build": "unbuild"
  },
  "files": [
    "*.md",
    "dist/**/*.*",
    "!**/*.tsbuildinfo",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "@unispec/compiler": "workspace:^",
    "@unispec/core": "workspace:^",
    "tslib": "^2.6.2"
  },
  "nx": {
    "implicitDependencies": [
      "@unispec/compiler",
      "@unispec/core"
    ]
  },
  "optionalDependencies": {
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/swagger": "^7.3.1",
    "change-case": "^5.4.4"
  },
  "unbuild": {
    "externals": [
      "@nestjs/graphql",
      "@nestjs/swagger",
      "change-case"
    ]
  },
  "description": "Unispec Driver for NestJS.",
  "repository": {
    "type": "git",
    "url": "https://github.com/guesant/unispec.git"
  },
  "license": "LGPL-2.1 license",
  "keywords": [
    "unispec"
  ],
  "author": "Gabriel R. Antunes <gabrielrodantunes@gmail.com>",
  "bugs": {
    "url": "https://github.com/guesant/unispec/issues"
  },
  "homepage": "https://github.com/guesant/unispec/tree/next/packages/driver-nestjs#readme"
}
