{
  "name": "@unispec/driver-nestjs",
  "version": "0.8.0",
  "sideEffects": false,
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guesant/unispec.git"
  },
  "license": "LGPL-2.1 license",
  "keywords": [
    "unispec"
  ],
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "lint:check": "eslint -c eslint.config.mjs",
    "lint:fix": "eslint -c eslint.config.mjs --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "coverage": "vitest run --coverage",
    "build:locally": "PKG_LOCALLY=true vite build",
    "dev:locally": "PKG_LOCALLY=true vite build -w"
  },
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "eslint": "~9.3.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "8.0.0-alpha.24"
  },
  "dependencies": {
    "@unispec/compiler": "workspace:*",
    "@unispec/core": "workspace:*",
    "tslib": "^2.6.2"
  },
  "optionalDependencies": {
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/swagger": "^7.3.1",
    "change-case": "^5.4.4"
  }
}
