{
  "name": "@unispec/compiler",
  "version": "0.26.1-next.9",
  "type": "module",
  "sideEffects": false,
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "release:base": "release-it -c .release-it.json",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "coverage": "vitest run --coverage",
    "build": "unbuild"
  },
  "files": [
    "*.md",
    "dist/**/*.*",
    "!**/*.tsbuildinfo",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "@unispec/core": "workspace:^",
    "tslib": "^2.6.3"
  },
  "unbuild": {
    "externals": [
      "@types/json-schema",
      "json-schema"
    ]
  },
  "description": "Unispec AST Nodes Compiler.",
  "repository": {
    "type": "git",
    "url": "https://github.com/guesant/unispec.git"
  },
  "license": "LGPL-2.1 license",
  "keywords": [
    "unispec",
    "unispec-compiler",
    "ast"
  ],
  "author": "Gabriel R. Antunes <gabrielrodantunes@gmail.com>",
  "bugs": {
    "url": "https://github.com/guesant/unispec/issues"
  },
  "homepage": "https://github.com/guesant/unispec/tree/next/packages/compiler#readme",
  "devDependencies": {
    "@types/json-schema": "^7.0.15"
  }
}
