{
  "name": "@unispec/core",
  "version": "0.26.1-next.2",
  "type": "module",
  "sideEffects": false,
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guesant/unispec.git"
  },
  "license": "LGPL-2.1 license",
  "keywords": [
    "unispec"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "lint:check": "eslint -c eslint.config.mjs",
    "lint:fix": "eslint -c eslint.config.mjs --fix",
    "release:base": "release-it -c .release-it.json",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "coverage": "vitest run --coverage",
    "build": "unbuild"
  },
  "dependencies": {
    "defu": "^6.1.4"
  },
  "unbuild": {
    "externals": [
      "type-fest"
    ]
  },
  "files": [
    "*.md",
    "dist/**/*.*",
    "!**/*.tsbuildinfo",
    "LICENSE",
    "README.md"
  ],
  "description": "Unispec AST Nodes Core.",
  "author": "Gabriel R. Antunes <gabrielrodantunes@gmail.com>",
  "bugs": {
    "url": "https://github.com/guesant/unispec/issues"
  },
  "homepage": "https://github.com/guesant/unispec/tree/next/packages/core#readme",
  "devDependencies": {
    "type-fest": "^4.19.0"
  }
}
